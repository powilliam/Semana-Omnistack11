{
    "env": {
        "es6": true,
        "node": true
    },
    "extends": [
        "standard",
        "eslint:recommended"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 2019,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "no-unused-vars": "off",
        "space-before-function-paren": "off",
        "indent": "off",
        "no-redeclare": "off"
    }
}